DROP TABLE IF EXISTS "space_session_summary";

CREATE TABLE "space_session_summary" (
	"space_session_id" BIGINT,

	"start_time" TIMESTAMP,
	"end_time" TIMESTAMP,
	"max_participants" INTEGER,

	CONSTRAINT "fk_space_session_summary__space_session"
		FOREIGN KEY ("space_session_id")
			REFERENCES "space_sessions"("id")
				ON DELETE CASCADE
);