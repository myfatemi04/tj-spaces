DROP TABLE IF EXISTS "space_worlds";

CREATE TABLE "space_worlds" (
	"id" BIGINT,
	"creator_id" BIGINT,
	"name" VARCHAR(255),
	"description" VARCHAR(255),
	"allows_templating" BOOLEAN,
	"download_count" BIGINT,


	CONSTRAINT "fk_space_world__creator"
		FOREIGN KEY ("creator_id")
			REFERENCES "users"("id")
				ON DELETE CASCADE
)